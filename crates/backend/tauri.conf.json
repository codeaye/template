{
  "build": {
    "devPath": "http://localhost:5173",
    "beforeDevCommand": "cd frontend && pnpm dev",
    "distDir": "../frontend/dist",
    "beforeBuildCommand": "cd frontend && pnpm build"
  },
  "package": {
    "productName": "template",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev",
      "shortDescription": "",
      "longDescription": "",
      "resources": [],
      "targets": "all",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "title": "Template",
        "width": 800,
        "height": 600,
        "fullscreen": false,
        "resizable": true
      }
    ]
  }
}
